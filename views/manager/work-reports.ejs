<main>
  <div class="grid">
    <% console.log(employees, 'employees v'); %>
    <% console.log(reports, 'reports v'); %>

    <% employees && employees.map(employee => { %>
    <div class="grid-item">
      <a class="image" href="/reports/:<%= employee.reports.report.reportId %>">
        <img src="<%= employee.image %>" alt="<%= employee._id %>" />
      </a>
      <p><%= employee.email %></p>
    </div>
    <% }) %>
  </div>

</main>

