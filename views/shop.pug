doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title #{shopTilte}
        link(rel="stylesheet", href="/css/main.css")
        link(rel="stylesheet", href="/css/product.css")
    body 
        header.mainHeader 
            nav.HeaderNav
                ul.navList
                    li.navListItem
                        a.active(href="/") shop
                    li.navListItem
                        a(href="/admin/add-product") add product
        main 
            if products.length > 0
                .productContainer
                    each product in products
                        article.card.productItem
                            header.cardHeader 
                                h1.productTitle #{product.title}
                            .cardImage 
                                img(alt="#{product.title}", src="data:image/png;base64,")
                            .cardContent 
                                h2.productPrice $19.99
                                p.productDescription Lorem ipsum dolor sit amet consectetur, adipisicing elit. Quae!
                            .cardActions 
                                button.btn add to cart
            else 
                h1 no product